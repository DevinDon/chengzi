        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/app-container/**/*.{ts,tsx,js,jsx}"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
